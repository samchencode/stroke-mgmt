<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('head') %>
  <%- include('style') %>
</head>

<body>
  <header class="template header">
    <h1 class="template header__title"><%- algorithm.getTitle() _%></h1>
    <i>Last Updated: <%- algorithm.getLastUpdated().toLocaleDateString(undefined, { year: 'numeric', month: 'long', day: 'numeric' }) _%></i>
  </header>
  <main class="template main">
    <p class="template body__paragraph">
      <%- algorithm.getBody() %>
    </p>
    <%- include('outcomeList', { algorithm }) %>
  </main>

  <%- include('script') %>
</body>

</html>